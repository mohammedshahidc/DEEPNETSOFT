    "outDir": "dist",
    "rootDir": "src",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "noEmit": false,
    "sourceMap": true,
    "removeComments": true,
    "resolveJsonModule": true

  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
